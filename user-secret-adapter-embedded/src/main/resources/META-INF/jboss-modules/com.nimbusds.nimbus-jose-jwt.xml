<modules xmlns="http://smartics.de/ns/jboss-modules-descriptor/1">
	<module name="com.nimbusds.nimbus-jose-jwt">
		<match>
			<includes>
				<include>
					<groupId>com.nimbusds</groupId>
					<artifactId>nimbus-jose-jwt</artifactId>
				</include>
			</includes>
		</match>
		<apply-to-dependencies>
			<dependencies>
				<match>
					<includes>
						<include>.+</include>
					</includes>
				</match>
				<apply>
					<export>true</export>
				</apply>
			</dependencies>
		</apply-to-dependencies>
	</module>
</modules>